{
  "name": "steal-socket.io",
  "version": "4.0.7",
  "description": "",
  "main": "io.js",
  "scripts": {
    "publish": "git push origin --tags && git push origin",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "test": "testee test/test.html --browsers firefox --reporter Spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stealjs/steal-socket.io.git"
  },
  "keywords": [
    "StealJS",
    "Socket.io"
  ],
  "author": "Matthew Phillips",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stealjs/steal-socket.io/issues"
  },
  "homepage": "https://github.com/stealjs/steal-socket.io",
  "dependencies": {
    "socket.io-client": "^1.7.1"
  },
  "devDependencies": {
    "can-fixture-socket": "^0.5.11",
    "can-zone": "^0.6.0",
    "steal": "^0.16.43",
    "steal-qunit": "^0.1.1",
    "testee": "^0.2.1"
  },
  "system": {
    "envs": {
      "server-development": {
        "map": {
          "socket.io-client/dist/socket.io": "@empty"
        }
      },
      "server-production": {
        "map": {
          "socket.io-client/dist/socket.io": "@empty"
        }
      }
    },
    "meta": {
      "socket.io-client/dist/socket.io": {
        "format": "global",
        "exports": "io"
      }
    },
    "npmDependencies": [
      "socket.io-client",
      "steal-qunit",
      "can-zone",
      "can-fixture-socket"
    ],
    "npmAlgorithm": "flat"
  }
}
