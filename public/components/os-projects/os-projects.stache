<can-component tag="bit-os-projects">
	
  <template>
  	{{#project}}
	<tr>
		<td>{{name}}</td>
		<td>Significance <input type="text" name="significance" {$value}="significance"/></td>
		<td><input type="checkbox" ($change)="toggle(.)" {{#if commissioned(.)}}checked{{/if}}/></td>
		<td>Total: {{total}}</td>
	</tr>
	{{/project}}
	</template>
</can-component>